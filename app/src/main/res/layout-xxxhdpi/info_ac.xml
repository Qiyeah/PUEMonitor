            android:id="@id/ac_attr5"
            style="@style/CellAttr"
            />
        <Spinner
            android:id="@id/ac_symbol5"
            style="@style/CellSymbol"
            />
        <EditText
            style="@style/Cell"
            android:text="100"
            android:id="@id/ac_per5"/>
    </TableRow>
</TableLayout>