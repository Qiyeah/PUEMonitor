<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:orientation="vertical"
             android:stretchColumns="0,1,2,3,4,5">
    <TableRow android:layout_width="match_parent"
              android:layout_height="50dp"
        >

        <TextView
            style="@style/Column"
            android:text="通道号"
            />

        <TextView
            style="@style/Column"
            android:text="名称"
            />

        <TextView
            style="@style/Column"
            android:text="属性"
            />

        <TextView
            style="@style/Column"
            android:text="符号"
            />

        <TextView
            style="@style/Column"
            android:text="百分比"
            />
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="50dp"
        >
        <TextView
            style="@style/Row"
            android:text="1"
            android:id="@id/ac_route1"/>
        <EditText
            style="@style/Cell"
            android:id="@id/ac_name1"/>
        <Spinner
            android:id="@id/ac_attr1"
            style="@style/CellAttr"
            />
        <Spinner
            android:id="@id/ac_symbol1"
            style="@style/CellSymbol"
            />
        <EditText
            style="@style/Cell"
            android:text="100"
            android:id="@id/ac_per1"/>
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="50dp"
        >

        <TextView
            style="@style/Row"
            android:text="2"
            android:id="@id/ac_route2"/>
        <EditText
            style="@style/Cell"
            android:id="@id/ac_name2"/>
        <Spinner
            android:id="@id/ac_attr2"
            style="@style/CellAttr"
            />
        <Spinner
            android:id="@id/ac_symbol2"
            style="@style/CellSymbol"
            />
        <EditText
            style="@style/Cell"
            android:text="100"
            android:id="@id/ac_per2"/>
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="50dp"
        >

        <TextView
            style="@style/Row"
            android:text="3"
            android:id="@id/ac_route3"/>
        <EditText
            style="@style/Cell"
            android:id="@id/ac_name3"/>
        <Spinner
            android:id="@id/ac_attr3"
            style="@style/CellAttr"
            />
        <Spinner
            android:id="@id/ac_symbol3"
            style="@style/CellSymbol"
            />
        <EditText
            style="@style/Cell"
            android:text="100"
            android:id="@id/ac_per3"/>
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="50dp"
        >

        <TextView
            style="@style/Row"
            android:text="4"
            android:id="@id/ac_route4"/>
        <EditText
            style="@style/Cell"
            android:id="@id/ac_name4"/>
        <Spinner
            android:id="@id/ac_attr4"
            style="@style/CellAttr"
            />
        <Spinner
            android:id="@id/ac_symbol4"
            style="@style/CellSymbol"
            />
        <EditText
            style="@style/Cell"
            android:text="100"
            android:id="@id/ac_per4"/>
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="50dp"
        >

        <TextView
            style="@style/Row"
            android:text="5"
            android:id="@id/ac_route5"/>
